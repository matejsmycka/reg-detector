<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="IsExporterLogFolder" value="C:\Logs\IS_export\" />
    <add key="LogFileFormat" value="IS_export-{0:yyyyMMdd}.txt" />
    <add key="EventTemplate" value="{@event_object} {@event_state} {@error_message}" />
    <add key="ErrorTag" value="error_message" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
