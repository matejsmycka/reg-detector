<ApplicationDef>
	<Application>
		<Name>AAA Test Application</Name>
		<Description>Template Used for SCCM Application Packager, Used as a test application.</Description>
		<Publisher>ICS MU</Publisher>
		<AutoInstall>True</AutoInstall>
		<UserDocumentation>https://ics.muni.cz</UserDocumentation>
		<Icon></Icon>
	</Application>
	<Downloads>
		<Download DeploymentType="DeploymentType1">
			<URL>https://raw.githubusercontent.com/asjimene/SCCM-Application-Packager/master/README.md</URL>
			<DownloadFileName>README.MD</DownloadFileName>
			<DownloadVersionCheck>$Version = 1
            $FullVersion = 1
            </DownloadVersionCheck>
			<FullVersion></FullVersion>
			<Version></Version>
		</Download>
		<Download DeploymentType="DeploymentType2">
			<URL>https://raw.githubusercontent.com/asjimene/SCCM-Application-Packager/master/README.md</URL>
			<DownloadFileName>README.MD</DownloadFileName>
			<DownloadVersionCheck>$Version = 1
            $FullVersion = 1
            </DownloadVersionCheck>
			<FullVersion></FullVersion>
			<Version></Version>
		</Download>
	</Downloads>
	<DeploymentTypes>
		<DeploymentType Name="DeploymentType2">
			<DeploymentTypeName>TEST APP x64</DeploymentTypeName>
			<InstallationType>Script</InstallationType>
			<Comments>Silent Installer for Google Chrome</Comments>
			<Language>English</Language>
			<CacheContent>False</CacheContent>
			<BranchCache>True</BranchCache>
			<ContentFallback>True</ContentFallback>
			<OnSlowNetwork>Download</OnSlowNetwork>
			<InstallProgram>cmd /c msiexec.exe test.msi /qn &amp;&amp; DEL /Q "%public%\Desktop\Google Chrome.lnk"</InstallProgram>
			<UninstallCmd></UninstallCmd>
			<Force32bit>False</Force32bit>
			<InstallationBehaviorType>InstallForSystem</InstallationBehaviorType>
			<LogonReqType>WhetherOrNotUserLoggedOn</LogonReqType>
			<UserInteractionMode>Hidden</UserInteractionMode>
			<ReqUserInteraction>False</ReqUserInteraction>
			<EstRuntimeMins>15</EstRuntimeMins>
			<MaxRuntimeMins>30</MaxRuntimeMins>
			<RebootBehavior>BasedOnExitCode</RebootBehavior>
			<DetectionMethodType>CustomScript</DetectionMethodType>
			<ScriptLanguage>PowerShell</ScriptLanguage>
			<DetectionMethod>Write-Output 1</DetectionMethod>
            <Requirements>
				<RuleName>OS Architecture x64</RuleName>
			</Requirements>
			<Supersedence>True</Supersedence>
		</DeploymentType>
		<DeploymentType Name="DeploymentType1">
			<DeploymentTypeName>Requirements Template</DeploymentTypeName>
			<InstallationType>Script</InstallationType>
			<CacheContent>False</CacheContent>
			<BranchCache>True</BranchCache>
			<ContentFallback>True</ContentFallback>
			<OnSlowNetwork>Download</OnSlowNetwork>
			<InstallProgram>hostname.exe</InstallProgram>
			<UninstallCmd></UninstallCmd>
			<InstallationBehaviorType>InstallForSystem</InstallationBehaviorType>
			<LogonReqType>WhetherOrNotUserLoggedOn</LogonReqType>
			<UserInteractionMode>Hidden</UserInteractionMode>
			<EstRuntimeMins>15</EstRuntimeMins>
			<MaxRuntimeMins>60</MaxRuntimeMins>
			<RebootBehavior>BasedOnExitCode</RebootBehavior>
			<DetectionMethodType>CustomScript</DetectionMethodType>
			<ScriptLanguage>PowerShell</ScriptLanguage>
			<DetectionMethod>Write-Output 1</DetectionMethod>
            <Requirements>
				<RuleName>OS Architecture x86</RuleName>
			</Requirements>
			<Supersedence>True</Supersedence>
		</DeploymentType>
	</DeploymentTypes>
	<Distribution>
		<DistributeContent>True</DistributeContent>
	</Distribution>
	<Deployments TestFirst="True">
		<Deployment type="Test">
			<DeploySoftware>True</DeploySoftware>
			<DeployAction>Install</DeployAction>
			<DeployPurpose>Available</DeployPurpose>
			<UserNotification>DisplaySoftwareCenterOnly</UserNotification>
			<DeploymentCollection>WKS - SD - Google Chrome - TEST</DeploymentCollection>
		</Deployment>
		<Deployment>
			<DeploySoftware>True</DeploySoftware>
			<DeployAction>Install</DeployAction>
			<DeployPurpose>Available</DeployPurpose>
			<UserNotification>DisplaySoftwareCenterOnly</UserNotification>
			<DeploymentCollection>WKS - SD - Google Chrome - Available</DeploymentCollection>
		</Deployment>
		<Deployment>
			<DeploySoftware>True</DeploySoftware>
			<DeployAction>Install</DeployAction>
			<DeployPurpose>Required</DeployPurpose>
			<UserNotification>DisplaySoftwareCenterOnly</UserNotification>
			<DeploymentCollection>WKS - SD - Google Chrome - Required</DeploymentCollection>
		</Deployment>
	</Deployments>
</ApplicationDef>